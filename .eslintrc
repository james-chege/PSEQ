{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "codeFrame": true,
    "esversion": 6,
    "ecmaVersion":2015,
    "ecmaFeatures":{
      "jsx": true
    }
  }
}
